{
    "name": "Open Jira Ticket Toolbar",
    "description": "Open JIRA tickets with extension toolbar",
    "manifest_version": 2,
    "version": "0.1",

    "browser_action": {
        "default_popup": "index.html"
    },

    "options_ui":
    {
        "page": "options.html",
        "chrome_style": true
    },

    "permissions": [
        "storage"
    ]
}